<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zzq.democollection">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <service
            android:name=".ipc.MyService"
            android:enabled="true"
            android:exported="true"
            android:process="com.zzq.democollection.myservice"></service>

        <activity android:name=".ipc.ThirdActivity"
            android:process=":activity"/>

        <activity android:name=".ipc.SecondActivity"
            android:process="com.zzq.democollection.activity"
            android:label="@string/second_activity"/>

        <activity android:name=".ipc.FirstActivity"
            android:label="@string/first_activity"/>

        <activity android:name=".DetailActivity" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>